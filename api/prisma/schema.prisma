generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id    String @id @default(uuid())
  name  String @db.VarChar(64)
  email String @db.VarChar(64)

  @@map("users")
}

model Profile {
  id   String @id @default(uuid())
  name String @db.VarChar(64)

  @@map("profiles")
}
